{
  "name": "autobot",
  "version": "11.0.0",
  "description": "A simple Angular project for playing with cars... while learning Angular ;-)",
  "homepage": "https://academiabinaria.github.io/autobot/",
  "author": {
    "name": "Alberto Basalo",
    "email": "albertobasalo@agorabinaria.com",
    "url": "https://twitter.com/albertobasalo"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4203",
    "build": "ng build --prod --stats-json",
    "prepub": "ng build --prod --output-path docs --base-href https://academiabinaria.github.io/autobot/",
    "pub": "git add * && git commit -m 'pub' && git push",
    "prod": "ng build --prod && http-server ./dist/autobot -o -c-1",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "compodoc -p src/tsconfig.app.json -s",
    "stats": "webpack-bundle-analyzer dist/autobot/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0-rc.1",
    "@angular/common": "^7.0.0-rc.1",
    "@angular/compiler": "^7.0.0-rc.1",
    "@angular/core": "^7.0.0-rc.1",
    "@angular/forms": "^7.0.0-rc.1",
    "@angular/http": "^7.0.0-rc.1",
    "@angular/platform-browser": "^7.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^7.0.0-rc.1",
    "@angular/pwa": "^0.9.0-rc.2",
    "@angular/router": "^7.0.0-rc.1",
    "@angular/service-worker": "^7.0.0-rc.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "bulma": "^0.7.1",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "moment": "^2.22.2",
    "rxjs": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.9.0-rc.3",
    "@angular/cli": "^7.0.0-rc.3",
    "@angular/compiler-cli": "^7.0.0-rc.1",
    "@angular/language-service": "^7.0.0-rc.1",
    "@ngrx/schematics": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.2",
    "webpack-bundle-analyzer": "^3.0.2"
  }
}
